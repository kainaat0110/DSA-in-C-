class Solution {
    public boolean isPalindrome(int x) {
        int result, y ,z;
        result = 0;
        z = x;
        while(x>0){
            y = x%10;
            result = result * 10 + y;
            x = x/10;
        }
        if(z == result){
            return true;
        } else {
            return false;
        }
    }
}
