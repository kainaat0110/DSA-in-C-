// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#include <algorithm>
int sum(int arr[] , int n){
    int sm = 0;
    for(int i = 0; i<n ; i++){
        sm = sm + arr[i];
    }
    return sm;
}
int main() {

    int n,k;
    cin>>n;
    k = n;
    int arr[n];
    for(int i =0; i<n; i++){
        cin>>arr[i];
    }
    for(int i = 0; i<n;i++){
        for(int j = i; j<n; j++){
            int subarr[j - i + 1]; 
            for(int k = i; k <= j; k++) {
                subarr[k - i] = arr[k]; 
            } cout<<sum(subarr,j - i + 1)<<endl;
        } 
    }

    
    return 0;
}
