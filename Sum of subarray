// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#include <algorithm>

int main() {

    int n,sum;
    cin>>n;
    sum = 0;
    int arr[n];
    for(int i =0; i<n; i++){
        cin>>arr[i];
    }
    for(int i = 0; i<n;i++){
         sum = 0;
        for(int j = i; j<n; j++){
            sum = sum + arr[j];
            cout<<sum;
        } 
    }

    
    return 0;
}
