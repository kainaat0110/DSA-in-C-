// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#include <algorithm>

int main() {

    int n;
    int maximum = 0;
    
    cin>>n;
    int arr[n];
    for(int i =0; i<n; i++){
        cin>>arr[i];
    }
    for(int i = 0; i<n;i++){
        maximum = max(maximum,arr[i]);
        cout<<maximum<<endl;
    }

    
    return 0;
}
