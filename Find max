// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n;
    int maxn = 0;
    cout<<"Enter the size of array"<<endl;
    cin>>n;
    int max[n];
    cout<<"Enter the elements in array"<<endl;
    for(int i = 0; i < n ; i++){
        cin>>max[i];
    }
    
    for(int i = 0 ; i < n ; i++){
        if(maxn > max[i]){
            continue;
        } else {
            maxn = max[i];
        }
    }
    cout<<"Max number is "<<maxn;
    return 0;
}
