// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n,k,key;
    int maxn = 0;
    cout<<"Enter the size of array: "<<endl;
    cin>>n;
    int max[n];
    cout<<"Enter the elements in array: "<<endl;
    for(int i = 0; i < n ; i++){
        cin>>max[i];
    }
    cout<<"Enter the element you want to search: "<<endl;
    cin>>key;
    
    for(k = 0 ; k < n ; k++){
        if(max[k]==key){
            cout<<k;
            break;
        } else {
            continue;
        }
    }
    if(k==n){
        cout<<"Number not found!";
    }
    return 0;
}
