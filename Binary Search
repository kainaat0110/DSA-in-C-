// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int search(int max[], int n, int key){
    int s = 0;
    int e = n;
    int mid;
    while(s<=e){
        mid = (s+e)/2;
        if(max[mid] == key){
            return mid;
        } else if(max[mid]>key){
            e = mid - 1;
        } else {
            s = mid + 1;
        }
    }
    return -1;
}
int main() {
    int n,k,key;
    int maxn = 0;
    cout<<"Enter the size of array: "<<endl;
    cin>>n;
    int max[n];
    cout<<"Enter the elements in array: "<<endl;
    for(int i = 0; i < n ; i++){
        cin>>max[i];
    }
    cout<<"Enter the element you want to search: "<<endl;
    cin>>key;
    cout<<search(max,n,key);
    return 0;
}
